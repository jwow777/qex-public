{
  "env": { "browser": true, "es6": true },
  "extends": ["airbnb-base", "eslint:recommended", "plugin:react/recommended"],
  "parserOptions": { 
    "ecmaVersion": 2021,
    "jsx": true,
    "sourceType": "module",
    "allowImportExportEverywhere": true
   },
   "rules": {
    "linebreak-style": 0,
    "react/prop-types": "off",
    "no-confusing-arrow": ["error", {"allowParens": true}]
  }
}